{
  "name": "conversational-dark",
  "displayName": "Conversational Dark",
  "publisher": "nathenbyford",
  "description": "A dark theme with warm accents for Positron IDE and VS Code, inspired by modern AI chat interfaces.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.103.0"
  },
  "categories": [
    "Themes"
  ],
  "icon": "images/conversational_icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/ntypeathenbyford/conversational-dark"
  },
  "license": "MIT",
  "keywords": [
    "Dark theme",
    "Positron"
  ],
  "bugs": {
    "type": "git",
    "url": "https://github.com/nathenbyford/conversational-dark/issues"
  },
  "contributes": {
    "themes": [
      {
        "label": "conversational-dark",
        "uiTheme": "vs-dark",
        "path": "./themes/conversational-dark-color-theme.json"
      }
    ]
  },
  "scripts": {
    "build": "vsce package",
    "release:ovsx": "ovsx publish --pat $OVSX_PAT"
  },
  "devDependencies": {
    "@vscode/vsce": "^3.0.0"
  }
}
